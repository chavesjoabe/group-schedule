{
  "configurations": {
    "run": {
      "adapter": "vscode-node",
      "breakpoints": {
        "exception": {
          "all": "N",
          "uncaught": "N"
        }
      },
      "configuration": {
        "request": "launch",
        "type": "node",
        "stopOnEntry": false,
        "console": "integratedTerminal",
        "program": "${workspaceFolder}/main.js",
        "skipFiles": [
          "node_modules/*/.js",
          "<node_internals>/*/.js"
        ],
        "processId": "${processId}"
      }
    }
  }
}
